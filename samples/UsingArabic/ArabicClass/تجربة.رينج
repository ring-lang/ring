اطبع "السلام عليكم ورحمة الله وبركاته"
اطبع سطر_جديد

ن1 = انشاء نقطة 
ن1.س = 10
ن1.ص = 20
ن1.ع = 30

اطبع ن1.س

فئة نقطة

	س ص ع

	ضبط_الدوال(نفسي)

دالة معرفة_قيمة_س
	اطبع "تحاول الوصل الى قيمة س"
	اطبع سطر_جديد
	ارجع س

دالة اسناد_قيمة_س القيمة
	اطبع "تحاول تعديل قيمة س"
	اطبع سطر_جديد
	س = القيمة
