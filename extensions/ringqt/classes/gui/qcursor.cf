	pObject = (QCursor *) RING_API_GETCPOINTER(1,"QCursor");
	{
		QBitmap *pValue ; 
		pValue = new QBitmap() ;
		*pValue = pObject->bitmap(Qt::ReturnByValue);
		RING_API_RETMANAGEDCPOINTER(pValue,"QBitmap",ring_QBitmap_freefunc);
	}
}

